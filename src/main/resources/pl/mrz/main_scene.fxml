                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </VBox.margin></Button>
                  <HBox alignment="CENTER">
                     <children>
                        <Button fx:id="getButton" mnemonicParsing="false" onAction="#getClicked" text="Get" />
